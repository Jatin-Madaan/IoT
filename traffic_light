void setup() {
  pinMode(1,OUTPUT);
  pinMode(2,OUTPUT);
  pinMode(3,OUTPUT);
  // put your setup code here, to run once:

}

void loop() {
  digitalWrite(1,HIGH);
  digitalWrite(2,HIGH);
  digitalWrite(3,HIGH);
  delay(2000);
  digitalWrite(3,LOW);
  delay(2000);
  digitalWrite(2,LOW);
  delay(2000);
  digitalWrite(2,HIGH);
  delay(2000);
  // put your main code here, to run repeatedly:

}
